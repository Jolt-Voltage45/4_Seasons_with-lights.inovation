// ---------------------------------
// PROGMEM-SAFE LED SETTING FUNCTION
// ---------------------------------
void setLEDColors(const uint16_t *LEDs, uint16_t numLEDs, CRGB color) {
  for (uint16_t i = 0; i < numLEDs; i++) {
    uint16_t idx = pgm_read_word(&LEDs[i]);  // read LED index from flash
    leds[idx] = color;
  }
}

// ------------------------
// SETUP
// ------------------------
void setup() {
  delay(1000);
  FastLED.addLeds<WS2811, DATA_PIN, GRB>(leds, NUM_LEDS);
  FastLED.clear();
  FastLED.setBrightness(15);
  delay(500);
}

